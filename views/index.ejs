<!DOCTYPE html>
<html lang="sv">

<head>
    <meta charset="UTI-8">
    <meta name="viewport" content="width-device-width, initial-scale=1.0">
    <title>Kurslogg</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>

<body>
    <div class="container">
        <main>
            <h1>Kurslogg</h1>

            <div class="navbar">
                <a href="/">Startsida</a>
                <a href="/courses">Lägg till kurs</a>
                <a href="/about">Om uppgiften</a>
              </div>

<h2>Lästa kurser</h2>
<% if(locals.rows.length > 0) { %>
    <% locals.rows.forEach((course) => { %>
        <article>
            <p id="course"><%= course.coursecode %>, <%= course.coursename %></p>
            <p id="course-info"><a href=""<%= course.syllabus %>>Kursplan</a>, progression: <%= course.progression %></p>
            <p>
                <a id="delete" href="/delete/<%= course.id %>">Radera kurs</a>
            </p>
        </article>
        <% }); %>
<% } %>

    <footer>
        Moment 1<br>
        emfo2405
    </footer>

        </main>
    </div>


</body>

</html>